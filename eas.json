{
	"cli": {
		"version": ">= 16.26.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_ENV": "development"
			}
		},
		"preview": {
			"android": {
				"buildType": "apk"
			},
			"ios": {
				"simulator": true
			},
			"channel": "preview",
			"env": {
				"EXPO_PUBLIC_ENV": "staging"
			}
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"EXPO_PUBLIC_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
